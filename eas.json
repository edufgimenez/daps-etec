{
  "cli": {
    "version": ">= 10.0.2"
  },
  "build": {
    "release": {
      "android": {
        "image": "latest"  // Certifique-se de usar uma imagem de build mais recente.
      }
    },
    "development": {
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_KEY": "EXPO_PUBLIC_SUPABASE_KEY",
        "EXPO_PUBLIC_HERE_API_KEY": "EXPO_PUBLIC_HERE_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
