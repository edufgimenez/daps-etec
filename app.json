{
  "expo": {
    "name": "D.A.P.S",
    "slug": "daps",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splashimage.png",
      "resizeMode": "contain",
      "aspectRatio": "1.5",
      "backgroundColor": "#F0A901"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.edufgimenez.daps",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "scheme": "daps",
    "sdkVersion": "51.0.0",
  }
}